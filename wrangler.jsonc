{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "jconet-marble-images",
  "main": "src/index.ts",
  "compatibility_date": "2025-08-30",
  "compatibility_flags": ["nodejs_compat"],
  "placement": {
    "mode": "smart"
  },
  "observability": {
    "enabled": true,
    "logs": {
      "enabled": true
    }
  },
  "workers_dev": false,
  "preview_urls": false,
  "routes": [
    {
      "custom_domain": true,
      "pattern": "marble.jconet.co.uk"
    }
  ]
}
